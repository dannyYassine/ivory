{
    "name": "dannyyassine/dev",
    "type": "libary",
    "description": "High performant micro http PHP library",
    "keywords": [
        "ivory",
        "openswoole"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "dannyyassine/ivory": "*",
        "illuminate/database": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../src"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dev\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "extra": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}